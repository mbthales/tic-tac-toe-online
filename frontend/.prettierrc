{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "^dotenv/config$",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^[@]",
    "",
    "<TYPES>"
  ],
  "vueIndentScriptAndStyle": true
}